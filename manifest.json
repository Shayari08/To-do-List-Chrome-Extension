{
  "manifest_version": 3,
  "name": "Todo List Extension",
  "version": "1.0",
  "description": "A todo list with auto-archiving and task management.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
